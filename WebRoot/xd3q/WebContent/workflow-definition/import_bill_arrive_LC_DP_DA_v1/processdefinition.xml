<?xml version="1.0" encoding="UTF-8"?>

<process-definition  xmlns=""  name="import_bill_arrive_LC_DP_DA_v1">


	<start-state name="进口到单处理开始">
		<transition to="判断" name="提交"></transition>
	</start-state>


	<task-node name="部门经理确认">
	    <task name="部门经理确认"></task>
	    <transition to="更新到单状态为有效" name="接受该单据"></transition>
		<transition to="进口到单信息修改" name="不接受该单据"></transition>
	</task-node>

	<task-node name="进口到单信息修改">
	    <task name="进口到单信息修改"></task>
		<transition to="部门经理确认" name="重新接受"></transition>
		<transition to="更新到单状态为作废" name="退单"></transition>
		<transition to="风险管理部" name="提交风险管理部"></transition>
	</task-node>

	<node name="更新到单状态为有效">
		<action class="com.infolion.sapss.workflow.payment.PickListInfoChangeSucessfulActionHandler" config-type="bean"></action>	
		<transition to="结束"></transition>
	</node>

	<node name="更新到单状态为作废">
		<action class="com.infolion.sapss.workflow.payment.PickListInfoChangeFailureActionHandler" config-type="bean"></action>	
		<transition to="结束（不接受不符）"></transition>
	</node>

	<task-node name="风险管理部">
		<task name="风险管理部"></task>
		<transition to="部门经理确认" name="提交"></transition>
		<transition to="进口到单信息修改" name="退回"></transition>
	</task-node>

	<decision name="判断">
	    <handler config-type="bean"  class="com.infolion.sapss.workflow.XDSSCommonDecisionHandler"></handler>
		<transition to="风险管理部" name="非TT到单"></transition>
		<transition to="部门经理确认" name="TT到单"></transition>
	</decision>


	<end-state name="结束"></end-state>

	<end-state name="结束（不接受不符）"></end-state>


</process-definition>