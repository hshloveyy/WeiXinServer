<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="" name="inner_transfer_approve_v1">


	<start-state name="内部划拨申请">
		<transition to="资金部经理审批" name="提交资金部经理审批"></transition>
	</start-state>


	<task-node name="资金部经理审批">
		<task name="资金部经理审批"></task>
		<transition to="股份分管副总审批" name="提交分管副总审批"></transition>
		<transition to="更新状态为不通过" name="不通过"></transition>
		<transition to="申请人修改" name="退回修改"></transition>
	</task-node>

	<task-node name="股份分管副总审批">
		<task name="股份分管副总审批"></task>
		<transition to="出纳付款" name="通过"></transition>
		<transition to="更新状态为不通过" name="不通过"></transition>
	</task-node>

	<task-node name="出纳付款">
		<task name="出纳付款"></task>
		<transition to="机要室确认" name="付款完毕"></transition>
	</task-node>

	<task-node name="机要室确认">
		<task name="机要室确认"></task>
		<transition to="更新状态为通过" name="确认"></transition>
	</task-node>

	<node name="更新状态为通过">
		<action class="com.infolion.sapss.workflow.account.InnerTransferProcessSucessfulActionHandler">
		</action>
		<transition to="申请通过"></transition>
	</node>

	<node name="更新状态为不通过">
		<action class="com.infolion.sapss.workflow.account.InnerTransferProcessFaileActionHandler">
		</action>
		<transition to="申请不通过"></transition>
	</node>

	<task-node name="申请人修改">
		<task name="申请人修改"></task>
		<transition to="资金部经理审批" name="提交资金部经理审批"></transition>
	</task-node>


	<end-state name="申请通过"></end-state>

	<end-state name="申请不通过"></end-state>



</process-definition>