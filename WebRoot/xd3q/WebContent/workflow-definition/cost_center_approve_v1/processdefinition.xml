<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="" name="cost_center_approve_v1">
	<start-state name="成本中心申请">
		<transition to="子公司财务部审核" name="提交财务部审核"></transition>
	</start-state>


	<task-node name="子公司财务部审核">
		<task name="子公司财务部审核"></task>
		<transition to="子公司行政部修改" name="退回修改"></transition>
		<transition to="更新状态为不通过" name="不通过"></transition>
		<transition to="股份财务部审批" name="通过"></transition>
	</task-node>

	<task-node name="股份财务部审批">
		<task name="股份财务部审批"></task>
		<transition to="信息中心维护" name="通过"></transition>
		<transition to="更新状态为不通过" name="不通过"></transition>
	</task-node>

	<task-node name="子公司行政部修改">
		<task name="子公司行政部修改"></task>
		<transition to="子公司财务部审核" name="提交财务部审核"></transition>
	</task-node>
	
	<node name="更新状态为不通过">
		<action class="com.infolion.sapss.workflow.account.CostCenterProcessFaileActionHandler">
		</action>
		<transition to="申请不通过"></transition>
	</node>

	<task-node name="信息中心维护">
		<task name="信息中心维护"></task>
		<transition to="更新状态为通过" name="维护完毕"></transition>
	</task-node>
	
	<node name="更新状态为通过">
		<action class="com.infolion.sapss.workflow.account.CostCenterProcessSucessfulActionHandler">
		</action>
		<transition to="申请通过"></transition>
	</node>
	

	<end-state name="申请通过"></end-state>

	<end-state name="申请不通过"></end-state>

</process-definition>