<?xml version="1.0" encoding="UTF-8"?>

<process-definition  xmlns=""  name="interest_bill_v1">


	<start-state name="利息税开票申请">
		<transition to="部门经理审核"></transition>
	</start-state>


	<task-node name="部门经理审核">
	    <task name="部门经理审核"></task>
		<transition to="部门会计审核" name="提交部门会计审核"></transition>
		<transition to="修改" name="退回修改"></transition>
	</task-node>

	<task-node name="部门会计审核">
	    <task name="部门会计审核"></task>
		<transition to="外贸财务经理审核" name="提交财务经理审核"></transition>
		<transition to="修改" name="退回修改"></transition>
	</task-node>

	<task-node name="外贸财务经理审核">
	    <task name="外贸财务经理审核"></task>
		<transition to="证券部审核" name="同意开票"></transition>
		<transition to="修改" name="退回修改"></transition>
	</task-node>

	<task-node name="证券部审核">
	    <task name="证券部审核"></task>
		<transition to="股份公司副总审批" name="提交股份副总审批"></transition>
		<transition to="修改" name="退回修改"></transition>
	</task-node>

	<task-node name="股份公司副总审批">
	    <task name="股份公司副总审批"></task>
		<transition to="股份公司副总兼外贸总经理审批" name="提交股份副总兼外贸总经理审批"></transition>
		<transition to="修改" name="退回修改"></transition>
		<transition to="更新状态为不通过" name="不同意开票"></transition>
	</task-node>

	<task-node name="股份公司副总兼外贸总经理审批">
	    <task name="股份公司副总兼外贸总经理审批"></task>
		<transition to="业务人员确认办理" name="同意开票"></transition>
		<transition to="修改" name="修改"></transition>
		<transition to="更新状态为不通过" name="不同意开票"></transition>
	</task-node>

	<task-node name="修改">
	    <task name="修改"></task>
	    <transition to="部门经理审核" name="提交部门经理"></transition>
		<transition to="更新状态为不通过" name="作废申请"></transition>
	</task-node>

	<node name="更新状态为通过">
	    <action class="com.infolion.sapss.workflow.interestBill.InterestBillSucessfulActionHandler" config-type="bean"></action>
		<transition to="结束（通过）"></transition>
	</node>

	<node name="更新状态为不通过">
	    <action class="com.infolion.sapss.workflow.interestBill.InterestBillFailureActionHandler" config-type="bean"></action>
		<transition to="结束（不通过）"></transition>
	</node>

	<task-node name="业务人员确认办理">
	    <task name="业务人员确认办理"></task>
		<transition to="机要室盖章" name="提交机要室盖章"></transition>
	</task-node>

	<task-node name="机要室盖章">
	    <task name="机要室盖章"></task>
		<transition to="财务人员过帐并录入凭证号" name="已盖章"></transition>
	</task-node>

	<task-node name="财务人员过帐并录入凭证号">
	    <task name="财务人员过帐并录入凭证号"></task>
		<transition to="更新状态为通过" name="已过帐并已录入凭证号"></transition>
	</task-node>


	<end-state name="结束（通过）"></end-state>

	<end-state name="结束（不通过）"></end-state>


</process-definition>