<?xml version="1.0" encoding="UTF-8"?>

<process-definition  xmlns=""  name="home_credityf_v1">


	<start-state name="议付申请">
		<transition to="部门经理确认"></transition>
	</start-state>


	<task-node name="部门经理确认">
	    <task name="部门经理确认"></task>
		<transition to="资金部经理审核" name="确认提交"></transition>
		<transition to="修改申请信息" name="退回修改"></transition>
		<transition to="更新状态为不通过" name="作废"></transition>
	</task-node>

	<task-node name="资金部经理审核">
        <task name="资金部经理审核"></task>
		<transition to="股份公司副总审批" name="提交股份公司副总审批"></transition>
		<transition to="修改申请信息" name="退回修改"></transition>
	</task-node>

	<task-node name="股份公司副总审批">
	    <task name="股份公司副总审批"></task>
		<transition to="资金部人员办理并填写实收金额及币别" name="同意"></transition>
		<transition to="资金部经理审核" name="退回资金部经理审核"></transition>
		<transition to="更新状态为不通过" name="不同意"></transition>
	</task-node>

	<task-node name="资金部人员办理并填写实收金额及币别">
	    <task name="资金部人员办理并填写实收金额及币别"></task>
		<transition to="更新状态为通过" name="已办理"></transition>
	</task-node>

	<node name="更新状态为不通过">
		<action class="com.infolion.sapss.workflow.export.ProcessChangeFailureActionHandler" config-type="bean"></action>	
		<transition to="审批不通过"></transition>
	</node>

	<node name="更新状态为通过">
		<action class="com.infolion.sapss.workflow.export.ProcessChangeSucessfulActionHandler" config-type="bean"></action>	
		<transition to="审批通过"></transition>
	</node>

	<task-node name="修改申请信息">
	    <task name="修改申请信息"></task>
		<transition to="部门经理确认" name="提交部门经理"></transition>
	</task-node>


	<end-state name="审批通过"></end-state>

	<end-state name="审批不通过"></end-state>


</process-definition>