<?xml version="1.0" encoding="UTF-8"?>
<process-definition xmlns="urn:jbpm.org:jpdl-3.2"
	name="storage_protocol_v3">
	<start-state name="仓储协议申请流程">
		<transition to="部门经理审批"></transition>
	</start-state>
	<task-node name="信达安总经理">
		<task name="信达安总经理"/>
		<transition to="设置申请通过" name="同意"></transition>
		<transition to="业务修改" name="退回修改"></transition>
	</task-node>

	<task-node name="贸管部审批">
		<task name="贸管部审批"/>
		<transition to="股份副总兼外贸公司总经理审批" name="同意"></transition>
		<transition to="业务修改" name="退回修改"></transition>
		<transition to="设置申请不通过" name="不同意"></transition>
	</task-node>

	<task-node name="股份副总兼外贸公司总经理审批">
		<task name="股份副总兼外贸公司总经理审批"/>
		<transition to="股份公司总经理" name="同意"></transition>
		<transition to="业务修改" name="退回修改"></transition>
		<transition to="设置申请不通过" name="不同意"></transition>
	</task-node>

	<node name="设置申请不通过">
		<action class="com.infolion.sapss.workflow.storageProtocol.StorageProtocolFailureActionHandler"></action>
		<transition to="申请不通过"></transition>
	</node>

	<node name="设置申请通过">
		<action class="com.infolion.sapss.workflow.storageProtocol.StorageProtocolSuccessActionHandler"></action>
		<transition to="申请通过"></transition>
	</node>

	<task-node name="业务修改">
		<task name="业务修改"/>
		<transition to="部门经理审批" name="提交部门经理"></transition>
		<transition to="设置申请不通过" name="作废"></transition>
	</task-node>

	<task-node name="部门经理审批">
		<task name="部门经理审批"></task>
		<transition to="外贸综合一部" name="同意"></transition>
		<transition to="业务修改" name="退回修改"></transition>
	</task-node>

	<task-node name="信达安贸管部">
	    <task name="信达安贸管部"></task>
		<transition to="信达安总经理" name="自有业务"></transition>
		<transition to="贸管部审批" name="非自有业务"></transition>
		<transition to="业务修改" name="退回修改"></transition>
	</task-node>

	<task-node name="外贸综合一部">
	    <task name="外贸综合一部"></task>
		<transition to="信达安贸管部" name="同意"></transition>
		<transition to="业务修改" name="退回修改"></transition>
	</task-node>

	<task-node name="股份公司总经理">
	    <task name="股份公司总经理"></task>
		<transition to="设置申请通过" name="同意"></transition>
		<transition to="设置申请不通过" name="不同意"></transition>
	</task-node>
	<end-state name="申请通过"/>

	<end-state name="申请不通过"/>
</process-definition>