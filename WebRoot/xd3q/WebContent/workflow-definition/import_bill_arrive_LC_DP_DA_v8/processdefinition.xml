<?xml version="1.0" encoding="UTF-8"?>

<process-definition  xmlns=""  name="import_bill_arrive_LC_DP_DA_v8">


	<start-state name="进口到单处理开始">
		<transition to="芜湖信达贸易总经理确认" name="提交部门经理确认"></transition>
	</start-state>


	<task-node name="芜湖信达贸易总经理确认">
	    <task name="芜湖信达贸易总经理确认"></task>
	    <transition to="更新到单状态为有效" name="接受该单据"></transition>
		<transition to="进口到单信息修改" name="不接受该单据"></transition>
	</task-node>

	<task-node name="进口到单信息修改">
	    <task name="进口到单信息修改"></task>
		<transition to="芜湖信达贸易总经理确认" name="重新接受"></transition>
		<transition to="更新到单状态为作废" name="退单"></transition>
	</task-node>

	<node name="更新到单状态为有效">
		<action class="com.infolion.sapss.workflow.payment.PickListInfoChangeSucessfulActionHandler" config-type="bean"></action>	
		<transition to="结束"></transition>
	</node>

	<node name="更新到单状态为作废">
		<action class="com.infolion.sapss.workflow.payment.PickListInfoChangeFailureActionHandler" config-type="bean"></action>	
		<transition to="结束（不接受不符）"></transition>
	</node>


	<end-state name="结束"></end-state>

	<end-state name="结束（不接受不符）"></end-state>


</process-definition>