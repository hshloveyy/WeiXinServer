<?xml version="1.0" encoding="UTF-8"?>

<process-definition xmlns="" name="subject_approve_v1">


	<start-state name="会计科目申请">
		<transition to="股份财务部审批" name="提交股份财务部审批"></transition>
	</start-state>


	<task-node name="股份财务部审批">
		<task name="股份财务部审批"></task>
		<transition to="子公司财务修改" name="退回修改"></transition>
		<transition to="更新状态为不通过" name="不通过"></transition>
		<transition to="信息中心录入" name="通过"></transition>
	</task-node>

	<task-node name="子公司财务修改">
		<task name="子公司财务修改"></task>
		<transition to="股份财务部审批" name="提交股份财务部审批"></transition>
	</task-node>

	<node name="更新状态为通过">
		<action class="com.infolion.sapss.workflow.account.SubjectProcessSucessfulActionHandler">
		</action>
		<transition to="申请通过"></transition>
	</node>

	<node name="更新状态为不通过">
		<action class="com.infolion.sapss.workflow.account.SubjectProcessFaileActionHandler">
		</action>
		<transition to="申请不通过"></transition>
	</node>

	<task-node name="信息中心录入">
		<task name="信息中心录入"></task>
		<transition to="更新状态为通过" name="录入完毕"></transition>
	</task-node>


	<end-state name="申请通过"></end-state>

	<end-state name="申请不通过"></end-state>


</process-definition>