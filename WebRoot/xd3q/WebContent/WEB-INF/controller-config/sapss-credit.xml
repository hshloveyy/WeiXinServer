<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<bean
		class="org.springframework.web.servlet.handler.SimpleUrlHandlerMapping">
		<property name="order" value="4" />
		<!-- 是否登录拦截器，针对每个请求进行过滤 -->
		<property name="interceptors">
			<list>
				<bean
					class="com.infolion.platform.console.sys.LogonStateInterceptor" />
			</list>
		</property>
		<property name="mappings">
			<props>
				<prop key="/creditEntryController.spr">creditEntryController</prop>
				<prop key="/creditArriveController.spr">creditArriveController</prop>
                <prop key="/creditEntryHisController.spr">creditEntryHisController</prop>
                <prop key="/creditArriveHisController.spr">creditArriveHisController</prop>              
			</props>
		</property>
	</bean>
	<!--信用证开证-->
	<bean id="creditEntryController"         
		class="com.infolion.sapss.credit.web.CreditEntryController"
		parent="baseMultiActionControllerXD" />
    <!--信用证到证-->
    <bean id="creditArriveController"         
		class="com.infolion.sapss.credit.web.CreditArriveController"
		parent="baseMultiActionControllerXD" />
     <!--信用证开证改证-->
	<bean id="creditEntryHisController"         
        class="com.infolion.sapss.credit.web.CreditEntryHisController"
        parent="baseMultiActionControllerXD" />
    <!--信用证收证改证-->
	<bean id="creditArriveHisController"         
        class="com.infolion.sapss.credit.web.CreditArriveHisController"
        parent="baseMultiActionControllerXD" />
	  	
        
</beans>
